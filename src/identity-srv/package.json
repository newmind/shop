{
  "name": "identity-srv",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "migration": "cross-env NODE_ENV=production sequelize-cli db:migrate",
    "migration:dev": "cross-env NODE_ENV=development sequelize-cli db:migrate",
    "seed": "cross-env NODE_ENV=production sequelize-cli db:seed:all",
    "seed:dev": "cross-env NODE_ENV=development sequelize-cli db:seed:all",
    "start": "yarn run migration && yarn run seed && node -r dotenv/config --experimental-modules ./src/index.mjs dotenv_config_path=./.env.production",
    "start:dev": "yarn run migration:dev && yarn run seed:dev && nodemon --exec node -r dotenv/config --experimental-modules ./src/index.mjs dotenv_config_path=./.env.development"
  },
  "dependencies": {
    "@sys.packages/db": "^1.0.0",
    "@sys.packages/server": "^1.0.0",
    "@sys.packages/sys.utils": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "2.8.1",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "nodemon": "1.19.1",
    "sequelize-cli": "^5.5.1"
  }
}

{
  "name": "product-proxy",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "migration": "cross-env NODE_ENV=production sequelize-cli db:migrate",
    "migration:dev": "cross-env NODE_ENV=development sequelize-cli db:migrate",
    "seed": "cross-env NODE_ENV=production sequelize-cli db:seed:all",
    "seed:dev": "cross-env NODE_ENV=development sequelize-cli db:seed:all",
    "start": "yarn run migration && node -r dotenv/config --experimental-modules ./src/index.mjs dotenv_config_path=./.env.production",
    "start:dev": "npx yarn run migration:dev && nodemon --exec node -r dotenv/config --experimental-modules ./src/index.mjs dotenv_config_path=./.env.development"
  },
  "dependencies": {
    "@sys.packages/db": "^1.0.0",
    "@sys.packages/rabbit": "^1.0.0",
    "@sys.packages/server": "^1.0.0",
    "@sys.packages/socket.io": "^1.0.0",
    "@sys.packages/sys.utils": "^1.0.0",
    "busboy": "^0.3.1",
    "dotenv": "8.1.0"
  },
  "devDependencies": {
    "cross-env": "5.2.1",
    "nodemon": "1.19.1",
    "sequelize-cli": "^5.5.1"
  }
}
